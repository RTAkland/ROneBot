package {{GROUP_ID}};

import cn.rtast.rob.milky.BotInstance;
import cn.rtast.rob.milky.MilkyBotFactory;
import cn.rtast.rob.milky.event.ws.packed.WebsocketConnectedEvent;
import cn.rtast.rob.milky.milky.MilkyListener;
import org.jetbrains.annotations.NotNull;

public class Main {
    public static class MyListener extends MilkyListener {
        @Override
        public void onConnected(@NotNull WebsocketConnectedEvent event) {
            System.out.println("连接成功~");
        }
    }

    public static void main(String[] args) {
        BotInstance bot = MilkyBotFactory.createBotBlocking("http://127.0.0.1:3000", "114514");
        bot.setListener(new MyListener());
        bot.joinBlocking();
    }
}